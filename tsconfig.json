{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "dist",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@data": ["types/data.ts"],
      "@functions/*": ["functions/*"]
    }
  },
  "include": ["src/**/*", "functions/**/*", "types/**/*"]
}
